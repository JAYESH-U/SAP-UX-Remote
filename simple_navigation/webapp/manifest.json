{
  "_version": "1.76.0",
  "sap.app": {
    "id": "ibm.sap.ux.simplenavigation",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.0",
      "toolsId": "641be8cf-74c1-4ff2-b19c-8e6f5f81539c"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.141.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ibm.sap.ux.simplenavigation.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ibm.sap.ux.simplenavigation.view",
        "async": true,
        "viewPath": "ibm.sap.ux.simplenavigation.view"
      },
      "routes": [
        {
          "name": "RouteCustomer",
          "pattern": ":?query:",
          "target": [
            "TargetCustomer"
          ]
        },
        {
          "name": "RouteSalesPage",
          "pattern": "sales/{id}",
          "target": [
            "TargetSales"
          ]
        }
      ],
      "targets": {
        "TargetCustomer": {
          "id": "Customer",
          "name": "Customer"
        },
        "TargetSales": {
          "id": "Sales",
          "name": "Sales"
        }
      }
    },
    "rootView": {
      "viewName": "ibm.sap.ux.simplenavigation.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
