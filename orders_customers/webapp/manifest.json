{
  "_version": "1.76.0",
  "sap.app": {
    "id": "ibm.sap.ux.orderscustomers",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.0",
      "toolsId": "3dd5a77e-37ae-4d27-b537-6b4598c6ac66"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.141.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ibm.sap.ux.orderscustomers.i18n.i18n"
        }
      },
      "": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "./model/orders.json",
        "settings": {
					"defaultBindingMode": "OneWay"
				}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ibm.sap.ux.orderscustomers.view",
        "async": true,
        "viewPath": "ibm.sap.ux.orderscustomers.view"
      },
      "routes": [
        {
          "name": "RouteOrders",
          "pattern": ":?query:",
          "target": [
            "TargetOrders"
          ]
        }
      ],
      "targets": {
        "TargetOrders": {
          "id": "Orders",
          "name": "Orders"
        }
      }
    },
    "rootView": {
      "viewName": "ibm.sap.ux.orderscustomers.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}